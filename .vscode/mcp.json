{
	"servers": {
		"filesystem": {
			"type": "stdio",
			"command": "cmd",
			"args": [
				"/c",
				"pnpm",
				"dlx",
				"@modelcontextprotocol/server-filesystem",
				"c:/proyectos/aleia-manjushry",
				"c:/proyectos"
			],
			"env": {
				"DISABLE_TELEMETRY": "true",
				"TELEMETRY_DISABLED": "1",
				"DO_NOT_TRACK": "1"
			}
		},
		"apify": {
			"type": "stdio",
			"command": "cmd",
			"args": [
				"/c",
				"pnpm",
				"dlx",
				"@apify/actors-mcp-server"
			],
			"env": {
				"APIFY_TOKEN": "${APIFY_TOKEN}",
				"DISABLE_TELEMETRY": "true",
				"TELEMETRY_DISABLED": "1",
				"DO_NOT_TRACK": "1"
			}
		},
		"firecrawl": {
			"type": "stdio",
			"command": "cmd",
			"args": [
				"/c",
				"pnpm",
				"dlx",
				"firecrawl-mcp"
			],
			"env": {
				"FIRECRAWL_API_KEY": "${FIRECRAWL_API_KEY}",
				"DISABLE_TELEMETRY": "true",
				"TELEMETRY_DISABLED": "1",
				"DO_NOT_TRACK": "1"
			}
		},
		"markitdown": {
			"type": "stdio",
			"command": "cmd",
			"args": [
				"/c",
				"pnpm",
				"dlx",
				"markitdown-mcp-npx"
			],
			"env": {
				"DISABLE_TELEMETRY": "true",
				"TELEMETRY_DISABLED": "1",
				"DO_NOT_TRACK": "1"
			}
		},
		"playwright": {
			"type": "stdio",
			"command": "cmd",
			"args": [
				"/c",
				"pnpm",
				"dlx",
				"@microsoft/mcp-server-playwright"
			],
			"env": {
				"DISABLE_TELEMETRY": "true",
				"TELEMETRY_DISABLED": "1",
				"DO_NOT_TRACK": "1"
			}
		},
		"docker mcp gateway run": {
			"type": "stdio",
			"command": "docker",
			"args": [
				"mcp",
				"gateway",
				"run"
			]
		},
		"sequential-thinking": {
		"command": "npx",
		"args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
		},
		"smart-thinking": {
		"command": "npx",
		"args": ["-y", "@Leghis/smart-thinking"]
		},
		"maxential-thinking": {
		"command": "npx",
		"args": ["-y", "@bam-devcrew/maxential-thinking-mcp"]

		}
	},
	"inputs": []
}
